<?xml version="1.0" encoding="utf-8"?>
<com.androidbook.kaoyanapp.listviewitems.CustomLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="50dp"
    android:background="@drawable/list_man_art_shape"
    android:orientation="vertical"

    >
    <RelativeLayout
        android:id="@+id/rl_collect"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        >

        <TextView
            android:id="@+id/display"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#2ECC71"
            android:gravity="center"
            android:text="发布"
            android:textColor="#ffffff"
            android:textSize="16sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"

        android:background="#2B2B2B" />

</com.androidbook.kaoyanapp.listviewitems.CustomLinearLayout>